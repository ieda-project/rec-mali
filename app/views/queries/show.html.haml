%section
  %h1
    = @query.title
    =# @query.case_status_key
  %h2 Graphique
  %div!= ziya_chart query_url(@query, format: :xml), size: '950x300'
  %table.results
    %thead
      %tr
        %th{:colspan => 4}
          Valeurs
          
    %tbody
      - @results.keys.sort.reverse.each do |k|
        %tr{ :class => cycle("odd", "even") }
          %td= k
          %td= @results[k]
